{
  "version": 2,
  "framework": null,
  "buildCommand": "cd ../.. && turbo run build --filter=@gp/api",
  "installCommand": "npm install -g pnpm@10.22.0 && cd ../.. && pnpm install",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
